<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="hacc2018.planthawaii.MainActivity"
    tools:showIn="@layout/app_bar_main">

    <EditText
        android:id="@+id/editText3"
        android:layout_width="93dp"
        android:layout_height="25dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="35dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="serif"
        android:hint="@string/home_browse"
        android:inputType="textPersonName"
        android:text="@string/home_camera"
        android:textAlignment="center"
        android:textColor="?attr/colorBackgroundFloating"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="93dp"
        android:layout_height="25dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="35dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="serif"
        android:hint="@string/home_report"
        android:inputType="textPersonName"
        android:text="@string/home_report"
        android:textAlignment="center"
        android:textColor="?attr/colorBackgroundFloating"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editText3"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton4" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="95dp"
        android:layout_height="96dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="21dp"
        android:clickable="true"
        android:focusable="true"
        android:onClick="openCamera"
        app:backgroundTint="?attr/colorControlHighlight"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        app:srcCompat="@android:drawable/ic_menu_camera" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="95dp"
        android:layout_height="96dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="166dp"
        android:clickable="true"
        android:focusable="true"
        android:onClick="openSearch"
        app:backgroundTint="?attr/colorControlHighlight"
        app:layout_constraintStart_toEndOf="@+id/floatingActionButton3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_search" />


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton3"
        android:layout_width="95dp"
        android:layout_height="96dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="166dp"
        android:clickable="true"
        android:focusable="true"
        android:onClick="openBrowse"
        app:backgroundTint="?attr/colorControlHighlight"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="?android:attr/colorBackgroundCacheHint"
        app:srcCompat="@android:drawable/ic_menu_gallery" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton4"
        android:layout_width="95dp"
        android:layout_height="96dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="54dp"
        android:clickable="true"
        android:focusable="true"
        android:onClick="openReport"
        app:backgroundTint="?attr/colorControlHighlight"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/floatingActionButton"
        app:layout_constraintTop_toBottomOf="@+id/editText2"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="93dp"
        android:layout_height="25dp"
        android:layout_marginStart="102dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="54dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="serif"
        android:hint="@string/home_search"
        android:inputType="textPersonName"
        android:text="@string/home_search"
        android:textAlignment="center"
        android:textColor="?attr/colorBackgroundFloating"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/editText"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton2" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="93dp"
        android:layout_height="25dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="25dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="serif"
        android:hint="@string/home_browse"
        android:inputType="textPersonName"
        android:text="@string/home_browse"
        android:textAlignment="center"
        android:textColor="?attr/colorBackgroundFloating"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton3" />

</android.support.constraint.ConstraintLayout>
